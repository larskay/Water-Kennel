<div class="adminheader">Treatments</div>
<%= link_to "New Treatment", new_treatment_path, :class => "btn btn-medium btn-primary" %>
<% @treatments.each do |treatment| %>
<table class="table table-striped">
  <tbody>
    <tr><th><i class="icon-th-list"></i>Date</th><td><%= treatment.date %></td>
    <th><i class="icon-th-list"></i>Full Treatment</th><td><%= treatment.full_treatment %></td></tr>
    <tr><th><i></i>Bath</th><td><%= treatment.bath %></td>
    <tr><th><i></i>Hair Dry</th><td><%= treatment.hair_dry %></td></tr>
    <tr><th><i></i>Ears</th><td><%= treatment.ears %></td>
    <tr><th><i></i>Teeth</th><td><%= treatment.teeth %></td>
    <tr><th><i></i>Claws</th><td><%= treatment.claws %></td>
    <tr><th><i></i>Shaving</th><td><%= treatment.shaving %></td>
    <tr><th><i></i>Napping</th><td><%= treatment.napping %></td>
    <tr><th><i></i>Brush</th><td><%= treatment.brush %></td>
    <tr><th><i></i>Form Cut</th><td><%= treatment.form_cut %></td>
    <tr><th><i></i>Walk</th><td><%= treatment.walk %></td>
    <tr><th><i></i>Minutes</th><td><%= treatment.minutes %></td>
    <tr><th><i></i>Price</th><td><%= treatment.price %></td>
  </tbody>
</table>
    <% end %>
    <%= will_paginate @treatment, renderer: BootstrapPagination::Rails %>
</div>
